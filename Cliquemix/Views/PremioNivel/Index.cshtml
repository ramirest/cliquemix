        <th>
            @Html.DisplayNameFor(model => model.dsPremio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.premioImg)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.tituloNivelPremio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dsPremio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.premioImg)
        </td>
